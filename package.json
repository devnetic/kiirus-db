{
  "name": "@devnetic/kiirus-db",
  "version": "1.0.0",
  "description": "A document-oriented NoSQL database, written entirely in JavaScript. ",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "npm run clean && tsc",
    "compile:watch": "npm run compile -- -w",
    "coverage": "nyc ava",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "posttest": "npm run lint",
    "prepare": "npm run compile",
    "prepublishOnly": "npm run build && npm run lint && npm run coverage",
    "pretest": "npm run compile",
    "release": "standard-version",
    "start": "node lib/index.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "ts-standard": {
    "ignore": [
      "lib",
      "examples"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devnetic/kiirus-db.git"
  },
  "keywords": [
    "nosql",
    "database",
    "document-oriented",
    "storage",
    "portable",
    "db"
  ],
  "author": "Álvaro José Agámez Licha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devnetic/kiirus-db/issues"
  },
  "homepage": "https://github.com/devnetic/kiirus-db#readme",
  "dependencies": {
    "@devnetic/load-env": "^1.1.0",
    "@devnetic/utils": "^1.2.1",
    "fastify": "^3.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/format": "^11.0.0",
    "@types/node": "^14.14.11",
    "ava": "^3.14.0",
    "gts": "^3.0.3",
    "husky": "^4.3.5",
    "nyc": "^15.1.0",
    "standard-version": "^9.0.0",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.0.3"
  }
}

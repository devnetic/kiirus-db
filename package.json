  "homepage": "https://github.com/devnetic/kiirus-db#readme",
  "dependencies": {
    "@devnetic/cli": "^1.1.0",
    "@devnetic/fetch": "^1.0.1",
    "@devnetic/server": "^1.1.0",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "ava": "^3.5.2",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "standard": "^14.3.3"
